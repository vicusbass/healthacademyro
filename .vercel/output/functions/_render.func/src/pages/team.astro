---
// Import the necessary components
import MainLayout from "@/layouts/MainLayout.astro";
import TeamSection from "@components/sections/TeamSection.astro";
import { SITE } from "@data/constants";

const pageTitle: string = `Echipa | ${SITE.title}`;
---

<MainLayout
  title={pageTitle}
  structuredData={{
    "@context": "https://schema.org",
    "@type": "WebPage",
    "@id": "https://health-academy.ro/team",
    url: "https://health-academy.ro/team",
    name: "Echipa | HealthAcademy",
    description: "Echipa noastra",
    isPartOf: {
      "@type": "WebSite",
      url: "https://health-academy.ro",
      name: "HealthAcademy",
      description:
        "HealthAcademy ofera servicii de inalt profesionalism de recuperare fizica.",
    },
    inLanguage: "ro-RO",
  }}
>
  <TeamSection />
</MainLayout>
